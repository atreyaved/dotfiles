@theme "~/.cache/wal/colors-rofi-dark.rasi"
* {
  background-color: #282C33;
  border-color: #2e343f;
  text-color: #8ca0aa;
  spacing: 0;
  width: 500px;
  height: 400px;
}

inputbar {
  children: [prompt,entry];
}

prompt {
  padding: 2px;
}

textbox {
  padding: 2px 2px;
}

entry {
  padding: 2px;
}

listview {
  cycle: false;
  margin: 0 0 -1px 0;
  scrollbar: false;
}

element {
  padding: 2px;
}

element selected {
  background-color: #2e343f;
}
